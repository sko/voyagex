RAILS_ENV=staging PIDFILE=./tmp/pids/resque-scheduler.pid BACKGROUND=yes rake resque:scheduler

ps aux | grep resque
resque_pids=`cat ./tmp/pids/resque-scheduler.pid && echo -n " " && cat ./tmp/pids/resque-worker.pid`
echo $resque_pids
kill $resque_pids
ps aux | grep resque

